{
  "name": "dk.globalgoals.org",
  "version": "0.0.1",
  "description": "The Official Website for The United Nation's Global Goals in Denmark",
  "main": "index.js",
  "scripts": {
    "test": "standard && dependency-check package.json",
    "deploy": "npx now -T codeandconspire && npx now alias -T codeandconspire",
    "start": "node server.js"
  },
  "documentify": {
    "transform": [
      [
        "./lib/document.js",
        {
          "order": "end"
        }
      ]
    ]
  },
  "browserify": {
    "transform": [
      [
        "sheetify",
        {
          "use": [
            [
              "sheetify-postcss",
              {
                "plugins": [
                  "autoprefixer",
                  "postcss-clean"
                ]
              }
            ]
          ]
        }
      ]
    ]
  },
  "now": {
    "alias": "dk.globalgoals.org",
    "env": {
      "NODE_ENV": "production",
      "PRISMIC_VERDENSMAALENE_SECRET": "@prismic-verdensmaalene-secret",
      "CLOUDFLARE_VERDENSMAALENE_ZONE": "@cloudflare-verdensmaalene-zone",
      "CLOUDFLARE_CODEANDCONSPIRE_EMAIL": "@cloudflare-codeandconspire-email",
      "CLOUDFLARE_CODEANDCONSPIRE_KEY": "@cloudflare-codeandconspire-key"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codeandconspire/dk.globalgoals.org.git"
  },
  "author": "code and conspire",
  "license": "CC-BY",
  "bugs": {
    "url": "https://github.com/codeandconspire/dk.globalgoals.org/issues"
  },
  "homepage": "https://github.com/codeandconspire/dk.globalgoals.org#readme",
  "dependencies": {
    "autoprefixer": "^9.1.1",
    "cccpurge": "^1.4.0",
    "choo": "^6.13.0",
    "choo-service-worker": "^2.4.0",
    "dedent": "^0.7.0",
    "dotenv": "^6.0.0",
    "focus-visible": "^4.1.5",
    "geoip-lite": "^1.3.2",
    "hstream": "^1.2.0",
    "intune": "^1.0.0",
    "jalla": "^0.5.1",
    "koa-body": "^4.0.4",
    "koa-compose": "^4.1.0",
    "koa-route": "^3.2.0",
    "mapbox-gl": "^0.48.0",
    "nanolru": "^1.0.0",
    "nanoraf": "^3.1.0",
    "normalize.css": "^8.0.0",
    "postcss-clean": "^1.1.0",
    "postcss-focus-visible": "^3.0.0",
    "prismic-element": "^1.0.0",
    "prismic-javascript": "^1.5.0",
    "prismic-richtext": "^0.4.2",
    "sheetify": "^7.3.3",
    "sheetify-postcss": "^1.2.0",
    "split-require": "^3.1.1"
  },
  "devDependencies": {
    "choo-devtools": "^2.5.1",
    "dependency-check": "^3.2.0",
    "standard": "^12.0.0"
  }
}
