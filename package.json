{
  "name": "verdensmaal.org",
  "version": "1.0.0",
  "description": "The Official Website for The United Nation's Global Goals in Denmark",
  "main": "index.js",
  "scripts": {
    "purge": "node -r dotenv/config -e \"require('./lib/purge')(['/sw.js'], console.log)\"",
    "test": "standard && dependency-check package.json",
    "deploy": "npx now -T codeandconspire && npx now alias -T codeandconspire",
    "start": "node server.js",
    "build": "jalla build index.js --sw sw.js"
  },
  "documentify": {
    "transform": [
      [
        "./lib/document.js",
        {
          "order": "end"
        }
      ]
    ]
  },
  "browser": {
    "./lib/scrape": "./lib/scrape/browser",
    "./lib/analytics": "./lib/analytics/browser"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "now": {
    "alias": "www.verdensmaal.org",
    "env": {
      "NODE_ENV": "production",
      "POLYFILL_FEATURES": "default-3.3,Array.prototype.find,Array.prototype.findIndex,Array.prototype.includes",
      "PRISMIC_SECRET": "@prismic-verdensmaal-secret",
      "CLOUDFLARE_ZONE": "@cloudflare-verdensmaal-zone",
      "CLOUDFLARE_EMAIL": "@cloudflare-codeandconspire-email",
      "CLOUDFLARE_KEY": "@cloudflare-codeandconspire-key",
      "CREATESEND_KEY": "@createsend-verdensmaal-key",
      "CREATESEND_LIST": "@createsend-verdensmaal-list",
      "CLOUDINARY_SECRET": "@cloudinary-secret",
      "CLOUDINARY_KEY": "@cloudinary-key",
      "ANALYTICS_VIEW_ID": "@analytics-verdensmaal-id",
      "GOOGLE_PRIVATE_KEY": "@google-verdensmaal-key",
      "GOOGLE_CLIENT_EMAIL": "@google-verdensmaal-email"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codeandconspire/verdensmaal.org.git"
  },
  "author": "code and conspire",
  "license": "CC-BY",
  "bugs": {
    "url": "https://github.com/codeandconspire/verdensmaal.org/issues"
  },
  "homepage": "https://github.com/codeandconspire/verdensmaal.org#readme",
  "dependencies": {
    "archiver": "^3.1.1",
    "cccpurge": "^2.0.0",
    "choo": "^6.13.3",
    "choo-lazy-view": "^1.0.2",
    "choo-service-worker": "^2.4.0",
    "cloudinary": "^1.14.0",
    "country-region-data": "^1.4.6",
    "date-fns": "^1.30.1",
    "dedent": "^0.7.0",
    "dotenv": "^6.2.0",
    "focus-visible": "^4.1.5",
    "geoip-lite": "^1.3.7",
    "googleapis": "^42.0.0",
    "hstream": "^1.2.0",
    "ics": "^2.16.0",
    "intune": "^1.0.0",
    "jalla": "0.12.0-7",
    "koa-body": "^4.1.1",
    "koa-compose": "^4.1.0",
    "koa-route": "^3.2.0",
    "mapbox-gl": "^0.52.0",
    "metascraper": "^5.6.6",
    "metascraper-date": "^5.6.6",
    "metascraper-description": "^5.6.6",
    "metascraper-image": "^5.6.6",
    "metascraper-publisher": "^5.6.6",
    "metascraper-title": "^5.6.6",
    "metascraper-url": "^5.6.6",
    "nanolru": "^1.0.0",
    "nanoraf": "^3.1.0",
    "node-fetch": "^2.6.0",
    "normalize.css": "^8.0.1",
    "postcss-custom-properties": "^8.0.11",
    "postcss-focus-visible": "^3.0.0",
    "postcss-url": "^8.0.0",
    "prismic-element": "^1.0.0",
    "prismic-javascript": "^2.1.1",
    "prismic-richtext": "^0.4.2",
    "slugify": "^1.3.4",
    "smoothscroll-polyfill": "^0.4.4"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "choo-devtools": "^2.5.1",
    "dependency-check": "^3.4.1",
    "standard": "^10.0.3"
  }
}
