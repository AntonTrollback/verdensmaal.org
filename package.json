{
  "name": "verdensmaal.org",
  "version": "1.0.0",
  "description": "The Official Website for The United Nation's Global Goals in Denmark",
  "main": "index.js",
  "scripts": {
    "purge": "node -r dotenv/config -e \"require('./lib/purge')('../index', ['/sw.js'], console.log)\"",
    "test": "standard && dependency-check package.json",
    "deploy": "npx now -T codeandconspire && npx now alias -T codeandconspire",
    "start": "node server.js",
    "build": "node build.js"
  },
  "engines": {
    "node": ">=10"
  },
  "documentify": {
    "transform": [
      [
        "./lib/document.js",
        {
          "order": "end"
        }
      ]
    ]
  },
  "browser": {
    "./lib/scrape": "./lib/scrape/browser",
    "./lib/analytics": "./lib/analytics/browser"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codeandconspire/verdensmaal.org.git"
  },
  "author": "code and conspire",
  "license": "CC-BY",
  "bugs": {
    "url": "https://github.com/codeandconspire/verdensmaal.org/issues"
  },
  "homepage": "https://github.com/codeandconspire/verdensmaal.org#readme",
  "dependencies": {
    "archiver": "^3.1.1",
    "cccpurge": "^2.0.0",
    "choo": "^7.0.0",
    "choo-lazy-view": "^1.0.2",
    "choo-service-worker": "^3.0.0",
    "cloudinary": "^1.14.0",
    "country-region-data": "^1.4.6",
    "date-fns": "^1.30.1",
    "dedent": "^0.7.0",
    "dotenv": "^6.2.0",
    "dropcss": "git+https://github.com/leeoniya/dropcss.git#2810f8736292fc929b71e287a24547a1b2ffc549",
    "focus-visible": "^4.1.5",
    "geoip-lite": "^1.3.7",
    "googleapis": "^42.0.0",
    "hstream": "^1.2.0",
    "ics": "^2.16.0",
    "intune": "^1.0.0",
    "jalla": "^1.0.0-24",
    "koa-body": "^4.1.1",
    "koa-compose": "^4.1.0",
    "koa-route": "^3.2.0",
    "mapbox-gl": "^0.52.0",
    "metascraper": "^5.6.6",
    "metascraper-date": "^5.6.6",
    "metascraper-description": "^5.6.6",
    "metascraper-image": "^5.6.6",
    "metascraper-publisher": "^5.6.6",
    "metascraper-title": "^5.6.6",
    "metascraper-url": "^5.6.6",
    "nanolru": "^1.0.0",
    "nanoraf": "^3.1.0",
    "node-fetch": "^2.6.0",
    "normalize.css": "^8.0.1",
    "postcss-custom-properties": "^8.0.11",
    "postcss-focus-visible": "^3.0.0",
    "postcss-url": "^8.0.0",
    "prismic-element": "^2.0.3",
    "prismic-javascript": "^2.1.1",
    "prismic-richtext": "^0.4.2",
    "slugify": "^1.3.4",
    "smoothscroll-polyfill": "^0.4.4"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "choo-devtools": "^3.0.3",
    "dependency-check": "^3.4.1",
    "standard": "^14.0.2"
  }
}
