{
  "version": 2,
  "alias": "www.verdensmaal.org",
  "env": {
    "NODE_ENV": "production",
    "POLYFILL_FEATURES": "es2015,es2016,es2017",
    "PRISMIC_SECRET": "@prismic-verdensmaal-secret",
    "CLOUDFLARE_ZONE": "@cloudflare-verdensmaal-zone",
    "CLOUDFLARE_EMAIL": "@cloudflare-codeandconspire-email",
    "CLOUDFLARE_KEY": "@cloudflare-codeandconspire-key",
    "CREATESEND_KEY": "@createsend-verdensmaal-key",
    "CREATESEND_LIST": "@createsend-verdensmaal-list",
    "CLOUDINARY_SECRET": "@cloudinary-secret",
    "CLOUDINARY_KEY": "@cloudinary-key",
    "ANALYTICS_VIEW_ID": "@analytics-verdensmaal-id",
    "GOOGLE_PRIVATE_KEY": "@google-verdensmaal-key",
    "GOOGLE_CLIENT_EMAIL": "@google-verdensmaal-email"
  },
  "build": {
    "env": {
      "NODE_ENV": "production",
      "NODEJS_HELPERS": "0"
    }
  },
  "routes": [
    {
      "src": "/.*",
      "dest": "/api/index.js"
    }
  ]
}
